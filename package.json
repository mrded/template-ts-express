{
  "name": "template-ts-express",
  "description": "Template for a webservice on typescript + express",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev src/index.ts",
    "build": "tsc",
    "prepare": "npm run build",
    "lint": "eslint 'src/**/*.?s'",
    "test": "jest",
    "format": "prettier --write 'src/**/*.?s'"
  },
  "author": "Dmitry Demenchuk @mrded",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "dotenv": "^16.0.2",
    "express": "^4.17.1",
    "http-errors": "^2.0.0",
    "jest-junit": "^14.0.1",
    "response-time": "^2.3.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/compression": "1.7.2",
    "@types/express": "4.17.13",
    "@types/http-errors": "^1.8.2",
    "@types/jest": "^28.1.8",
    "@types/node": "^18.7.14",
    "@types/response-time": "^2.3.4",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^28.1.3",
    "nock": "^13.0.5",
    "prettier": "^2.2.1",
    "supertest": "^6.0.1",
    "ts-jest": "^28.0.8",
    "ts-node-dev": "^2.0.0",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.6.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mrded/template-ts-express.git"
  }
}
